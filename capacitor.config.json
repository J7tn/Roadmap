{
  "appId": "com.careeratlas.roadmap",
  "appName": "Careering",
  "webDir": "dist",
  "server": {
    "androidScheme": "https"
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2500,
      "backgroundColor": "#ffffff",
      "showSpinner": false,
      "spinnerColor": "#3b82f6",
      "androidSplashResourceName": "splash",
      "splashFullScreen": true,
      "splashImmersive": true,
      "launchAutoHide": true
    },
    "StatusBar": {
      "style": "dark"
    }
  }
}
